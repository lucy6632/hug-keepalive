{
  "type": "module",
  "name": "hugging-face-docker-automatic-keep-alive",
  "version": "1.0.0",
  "description": "Hugging Face Space 自动保活工具 - 定时访问并自动刷新Cookie实现服务保活",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "docker:build": "docker build -t hf-keep-alive .",
    "docker:run": "docker run -d -e TARGET_URL=$TARGET_URL -e CURRENT_COOKIE=$CURRENT_COOKIE --name hf-keep-alive hf-keep-alive"
  },
  "keywords": [
    "huggingface",
    "keep-alive",
    "docker",
    "typescript",
    "space",
    "automatic"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cookie": "1.1.1"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/node": "^20.19.27",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  }
}
